          else return 0\r\n        },\r\n        total_porb(){\r\n            if (this.$store.state.data_result.porb){\r\n                return this.$store.state.data_result.porb[this.total_porb_length]*100}\r\n            else return [1]\r\n        },\r\n\r\n        myoption(){\r\n            var myoption = {\r\n                title: {\r\n                    text: '部署实时收益率'\r\n                },\r\n                series: [{\r\n                    type: 'gauge',\r\n                    axisLine: {\r\n                        lineStyle: {\r\n                            width: 30,\r\n                            color: [\r\n                                [0.3, '#67e0e3'],\r\n                                [0.7, '#37a2da'],\r\n                                [1, '#fd666d']\r\n                            ]\r\n                        }\r\n                    },\r\n                    pointer: {\r\n                        itemStyle: {\r\n                            color: 'auto'\r\n                        }\r\n                    },\r\n                    axisTick: {\r\n                        distance: -30,\r\n                        length: 8,\r\n                        lineStyle: {\r\n                            color: '#fff',\r\n                            width: 2\r\n                        }\r\n                    },\r\n                    splitLine: {\r\n                        distance: -30,\r\n                        length: 30,\r\n                        lineStyle: {\r\n                            color: '#fff',\r\n                            width: 4\r\n                        }\r\n                    },\r\n                    axisLabel: {\r\n                        color: 'auto',\r\n                        distance: 10,\r\n                        fontSize: 15\r\n                    },\r\n                    detail: {\r\n                        valueAnimation: true,\r\n                        formatter: '{value} %',\r\n                        color: 'auto',\r\n                        fontSize: 15\r\n                    },\r\n                    data: [{\r\n                        value: this.total_porb\r\n                    }]\r\n                }]\r\n            };\r\n            return myoption\r\n        }\r\n    },\r\n    watch:{\r\n        total_porb(newVal){\r\n            return newVal\r\n        },\r\n        total_porb_length(newVal){\r\n            return newVal\r\n        },\r\n        total_porb_current(newVal){\r\n            return newVal\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n    .xpanel-wrapper-32  {height:50%;}\r\n    .xpanel-wrapper {padding-bottom:15px;box-sizing:border-box;}\r\n    .fill-h {height:100%;min-height:100%;}\r\n    .xpanel {\r\n        padding:15px;\r\n        height:100%;\r\n        min-height:170px;\r\n        background:url(\"../../../assets/panel.png\") center no-repeat;\r\n        background-size:100% 100%;\r\n        box-sizing:border-box;\r\n    }\r\n</style>\r\n"],"sourceRoot":"src/components/showpage/rightchart"}]}